<main>

<a href="/newuser">Add a username</a>
<a href="/create">Add a post</a>

<h1>Life in the 38th State</h1>

{{#each blog}}
  <div class="container">
    <a href="{{this.id}}"><h2>{{this.title}}</h2></a>
    <p>{{this.content}}</p>

    <img class="image" src="{{this.image}}" alt="" />
    <p>{{this.username}}</p>
    </div>
  {{/each}}

</main>
